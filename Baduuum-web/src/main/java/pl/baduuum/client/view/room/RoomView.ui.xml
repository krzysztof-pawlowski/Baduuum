<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:pl.baduuum.client.view.widgets">
	<g:DecoratorPanel>
		<g:VerticalPanel>
			<g:HTMLPanel>
				<c:HeaderWidgetView ui:field="header" />
			</g:HTMLPanel>
			
			<g:MenuBar title="Account" ui:field="menuBar"
				animationEnabled="true" autoOpen="true">
				<g:MenuItem text="HOME" ui:field="buttonHome" />
				<g:MenuItem text="CENNIK" ui:field="buttonPrice" />
				<g:MenuItem text="GRAFIK" ui:field="buttonCalendar" />
				<g:MenuItem text="SALA" ui:field="buttonRoom" />
				<g:MenuItem text="REZERWACJA" ui:field="buttonReservation" />
				<g:MenuItem text="GALERIA" ui:field="buttonGallery">
					<g:MenuBar vertical="true">
						<g:MenuItem text="SALA TWIN PEAKS" ui:field="buttonGalleryRoom" />
						<g:MenuItem text="BUDOWA SALI" ui:field="buttonGalleryBuild" />
					</g:MenuBar>
				</g:MenuItem>
				<g:MenuItem text="REGULAMIN" ui:field="buttonRegulations" />
				<g:MenuItem text="FAQ" ui:field="buttonFAQ" />
				<g:MenuItem text="KONTAKT" ui:field="buttonContact" />
			</g:MenuBar>

			<g:HorizontalPanel>
				<g:HTML>
					room
				</g:HTML>
			</g:HorizontalPanel>

			<g:HTMLPanel>
				<c:FooterWidgetView ui:field="footer" />
			</g:HTMLPanel>
		</g:VerticalPanel>
	</g:DecoratorPanel>
</ui:UiBinder> 